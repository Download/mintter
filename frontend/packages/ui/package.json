{
  "name": "@mintter/ui",
  "version": "0.1.0",
  "private": true,
  "types": "./types/index",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "lint": "eslint .",
    "watch": "tamagui-build --watch",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json,js}\" --check --ignore-path ../../../.prettierignore",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "test": "echo TODO",
    "typecheck": "tsc --noEmit",
    "generate": "tamagui generate-themes ./src/themes.ts ./src/generated-themes.ts"
  },
  "dependencies": {
    "@mintter/shared": "*",
    "@tamagui/animations-react-native": "1.69.0",
    "@tamagui/config": "1.69.0",
    "@tamagui/create-theme": "1.69.0",
    "@tamagui/lucide-icons": "1.69.0",
    "@tamagui/theme-builder": "1.69.0",
    "@tamagui/web": "1.69.0",
    "tamagui": "1.69.0"
  },
  "devDependencies": {
    "@mintter/prettier": "*",
    "@storybook/react": "^7.0.26",
    "@tamagui/build": "1.69.0",
    "@tamagui/cli": "1.69.0",
    "@tamagui/generate-themes": "1.69.0",
    "eslint-config-custom": "*"
  }
}
