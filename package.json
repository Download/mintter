{
  "name": "mintter",
  "version": "0.0.1",
  "description": "Mintter monorepo",
  "main": "frontend/www/index.js",
  "repository": "https://github.com/mintterteam/mintter",
  "author": "Mintter Team",
  "private": true,
  "scripts": {
    "changeset": "changeset",
    "test": "echo 1",
    "build:all": "yarn workspaces run build",
    "site:build": "yarn workspace @mintter/site build",
    "site:export": "yarn workspace @mintter/site export",
    "release": "yarn build:all",
    "dev": "yarn workspace @mintter/site dev",
    "publish:all": "yarn workspaces run npm:publish"
  },
  "workspaces": [
    "frontend/www",
    "frontend/packages/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.5.1",
    "@types/jest": "^25.1.3",
    "husky": "^4.2.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "yarn": "^1.19.0"
  }
}
